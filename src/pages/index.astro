---

import Layout from '../layouts/Layout.astro';
import AccordionComponent from '../components/AccordionComponent.astro'
import Cursor from '../components/Cursor.astro'
import Navbar from '../components/Navbar.astro';
import About from '../components/About.astro';
import Services from '../components/Services.astro';
import Profiles from '../components/Profiles.astro';
import Footer from '../components/Footer.astro';
import Process from '../components/Process.astro';
import Jobs from '../components/Jobs.astro';
---

<Layout title="Jupiter Design">
	<main class="w-full pt-9 flex flex-col items-center">
		<Navbar />
		<About />
		<Services/>
		<Profiles />
		<Jobs />
		<Process />
		<AccordionComponent />
		<Footer/>
		<Cursor />
