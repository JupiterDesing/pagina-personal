---

import Layout from '../layouts/Layout.astro';
import AccordionComponent from '../components/AccordionComponent.astro'
import Cursor from '../components/Cursor.astro'
import Navbar from '../components/Navbar.astro';
import About from '../components/About.astro';
import Services from '../components/Services.astro';
import Profiles from '../components/Profiles.astro';
import Footer from '../components/Footer.astro';
import Process from '../components/Process.astro';
import Jobs from '../components/Jobs.astro';

---

<Layout title="Jupiter Design">
	<main class="w-screen relative flex flex-col items-center overflow-hidden">
		
		<Navbar />
		<About />

	<Services />
	<Profiles />
	<Jobs />
	<Process />

		<AccordionComponent />
		<Footer/>
		<Cursor />
		
		<span class="absolute top-[-800px] left-[600px] bg-backgroundPrimary z-0 h-[1600px] w-[1600px] rounded-3xl opacity-100 "></span>
		<span class="absolute  min-h-[100vh] w-full backdrop-blur-xl"></span>
	</main>
</Layout>

